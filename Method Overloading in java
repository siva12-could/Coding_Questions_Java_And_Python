// Method Overloading 
class Main{
    static int Add(int a,int b){
        return a+b;
    }
    static double Add(double a,double b){
        return a+b;
    }
    public static void main(String[] args){
        System.out.println(Add(3,6));
        System.out.println(Add(11.6,12.6));
        
    }
}
